{
  "name": "@thebackyardflora/rexburg-flowers",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx playwright test --ui\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=3000 start-server-and-test start:mocks http://localhost:3000 \"npx playwright test\"",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "seed:square": "node --require dotenv/config ./scripts/seed-square"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@headlessui/react": "1.7.15",
    "@heroicons/react": "2.0.18",
    "@isaacs/express-prometheus-middleware": "1.2.1",
    "@mailchimp/mailchimp_marketing": "3.0.80",
    "@prisma/client": "4.15.0",
    "@remix-run/express": "1.17.0",
    "@remix-run/node": "1.17.0",
    "@remix-run/react": "1.17.0",
    "@remix-run/server-runtime": "1.17.0",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.2",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "isbot": "3.6.10",
    "morgan": "1.10.0",
    "prom-client": "14.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redis": "4.6.7",
    "square": "26.0.0",
    "tiny-invariant": "1.3.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@playwright/test": "1.35.1",
    "@remix-run/dev": "1.17.0",
    "@remix-run/eslint-config": "1.17.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/eslint": "8.40.2",
    "@types/express": "4.17.17",
    "@types/mailchimp__mailchimp_marketing": "3.0.9",
    "@types/morgan": "1.9.4",
    "@types/node": "18.16.18",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.31.4",
    "autoprefixer": "10.4.14",
    "c8": "8.0.0",
    "cookie": "0.5.0",
    "dotenv": "16.1.4",
    "esbuild": "0.18.3",
    "esbuild-register": "3.4.2",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "9.20.3",
    "msw": "1.2.2",
    "nock": "13.3.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prisma": "4.15.0",
    "start-server-and-test": "2.0.0",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "uuid": "9.0.0",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.4"
  },
  "engines": {
    "node": "20.1.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
